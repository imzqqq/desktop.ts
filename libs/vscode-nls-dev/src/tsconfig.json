{
	"compilerOptions": {
		"target": "es2017",
		"module": "commonjs",
		"lib": [
			"es2017"
		],
		"moduleResolution": "node",
		"sourceMap": true,
		"inlineSources": false,
		"declaration": true,
		"stripInternal": true,
		"strict": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"outDir": "../lib"
	}
}